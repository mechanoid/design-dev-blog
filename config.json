{
  "locals": {
    "url": "http://localhost:8080",
    "name": "design:dev",
    "owner": "Falk Hoppe",
    "description": "unfolding web technologies"
  },
  "plugins": [
    "./plugins/paginator.coffee",
    "./node_modules/wintersmith-stylus"
  ],
  "require": {
    "moment": "moment",
    "_": "underscore",
    "typogr": "typogr"
  },
  "jade": {
    "pretty": true
  },
  "markdown": {
    "smartLists": true,
    "smartypants": true
  },
  "paginator": {
    "perPage": 3
  },
  "stylus": {
    "dependencies": [
      "nib",
      "jeet",
      "rupture"
    ]
  }
}
